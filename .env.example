# Session secret (generate with: openssl rand -hex 64)
TUDUDI_SESSION_SECRET=your-secret-key-here

# Initial user credentials
TUDUDI_USER_EMAIL=admin@example.com
TUDUDI_USER_PASSWORD=your-secure-password

# Database file location (optional)
# DB_FILE=./backend/db/tududi.sqlite

# Server configuration (optional)
# PORT=3002
# HOST=0.0.0.0

# CORS allowed origins (optional)
# TUDUDI_ALLOWED_ORIGINS=http://localhost:8080,http://localhost:3002
