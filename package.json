{
  "name": "tududi-kanban-mvp",
  "version": "1.0.0",
  "description": "Self-hosted task management with kanban board and status tracking",
  "scripts": {
    "start": "npm run backend:start & npm run frontend:dev",
    "dev": "npm run frontend:dev",
    "build": "npm run frontend:build",
    "frontend:dev": "webpack serve --config webpack.config.js --hot",
    "frontend:build": "npm run clean && tsc --noEmit && webpack --config webpack.config.js",
    "backend:start": "cd backend && node app.js",
    "backend:dev": "cd backend && nodemon app.js",
    "db:init": "cd backend && node scripts/db-init.js",
    "clean": "rimraf dist"
  },
  "keywords": ["task-management", "kanban", "productivity"],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.25.7",
    "@babel/preset-env": "^7.25.7",
    "@babel/preset-react": "^7.25.7",
    "@babel/preset-typescript": "^7.25.7",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.15",
    "@swc/core": "^1.13.3",
    "@types/node": "^20.0.0",
    "@types/react": "^18.3.10",
    "@types/react-dom": "^18.3.0",
    "@types/react-router-dom": "^5.3.3",
    "autoprefixer": "^10.4.20",
    "babel-loader": "^9.2.1",
    "cross-env": "~7.0.3",
    "css-loader": "^7.1.2",
    "html-webpack-plugin": "^5.6.3",
    "nodemon": "~3.0.1",
    "postcss": "^8.4.47",
    "postcss-loader": "^8.1.1",
    "react-refresh": "^0.14.2",
    "rimraf": "^6.0.1",
    "sequelize-cli": "~6.6.2",
    "style-loader": "^4.0.0",
    "swc-loader": "^0.2.6",
    "tailwindcss": "^3.4.13",
    "ts-loader": "^9.5.1",
    "typescript": "^5.6.2",
    "webpack": "^5.95.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.1.0"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@heroicons/react": "^2.1.5",
    "bcrypt": "~6.0.0",
