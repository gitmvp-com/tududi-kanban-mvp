    "connect-session-sequelize": "~7.1.7",
    "cors": "~2.8.5",
    "dotenv": "~16.5.0",
    "express": "^4.21.2",
    "express-session": "~1.18.1",
    "helmet": "~8.1.0",
    "morgan": "~1.10.0",
    "nanoid": "^5.1.5",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.26.2",
    "sequelize": "~6.37.7",
    "sqlite3": "~5.1.7",
    "uuid": "~11.1.0",
    "zustand": "^5.0.3"
  }
}
